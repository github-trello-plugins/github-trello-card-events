{
  "name": "github-trello-card-events",
  "version": "1.0.0",
  "engines": {
    "node": "10.x.x"
  },
  "dependencies": {
    "@octokit/rest": "17.3.0",
    "body-parser": "1.19.0",
    "errorhandler": "1.5.1",
    "express": "4.17.1",
    "lodash": "4.17.15",
    "moment-timezone": "0.5.28",
    "request": "2.88.2",
    "request-promise": "4.2.5"
  },
  "devDependencies": {
    "@types/errorhandler": "1.5.0",
    "@types/express": "4.17.6",
    "@types/lodash": "4.14.149",
    "@types/moment-timezone": "0.5.13",
    "@types/request-promise": "4.1.46",
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsdoc": "23.0.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-security": "1.4.0",
    "husky": "3.1.0",
    "lint-staged": "10.1.4",
    "prettier": "2.0.4",
    "prettier-plugin-packagejson": "2.2.0",
    "typescript": "3.8.3"
  },
  "main": "dist/server.js",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "tsc",
    "lint": "prettier --write *.json .eslintrc.js .prettierrc.js && eslint --fix --ext .ts src",
    "dev": "tsc && node dist/server.js",
    "start": "node dist/server.js"
  },
  "lint-staged": {
    "./*.js": [
      "eslint --fix"
    ],
    "*.ts": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build && git add dist/"
    }
  },
  "repository": "https://github.com/mpirik/github-trello-card-events.git"
}
