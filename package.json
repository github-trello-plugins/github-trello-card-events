{
  "name": "github-trello-card-events",
  "version": "1.0.0",
  "engines": {
    "node": "10.x.x"
  },
  "dependencies": {
    "@octokit/rest": "18.0.0",
    "body-parser": "1.19.0",
    "errorhandler": "1.5.1",
    "express": "4.17.1",
    "lodash": "4.17.19",
    "moment-timezone": "0.5.31",
    "request": "2.88.2",
    "request-promise": "4.2.5"
  },
  "devDependencies": {
    "@types/errorhandler": "1.5.0",
    "@types/express": "4.17.7",
    "@types/lodash": "4.14.157",
    "@types/moment-timezone": "0.5.13",
    "@types/request-promise": "4.1.46",
    "@typescript-eslint/eslint-plugin": "3.6.1",
    "@typescript-eslint/parser": "3.6.1",
    "eslint": "7.4.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsdoc": "29.2.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-security": "1.4.0",
    "husky": "3.1.0",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "prettier-plugin-packagejson": "2.2.5",
    "typescript": "3.9.6"
  },
  "main": "dist/server.js",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "tsc",
    "lint": "prettier --write *.json .eslintrc.js .prettierrc.js && eslint --fix --ext .ts src",
    "dev": "tsc && node dist/server.js",
    "start": "node dist/server.js"
  },
  "lint-staged": {
    "./*.js": ["eslint --fix"],
    "*.ts": ["eslint --fix"]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build && git add dist/"
    }
  },
  "repository": "https://github.com/mpirik/github-trello-card-events.git"
}
