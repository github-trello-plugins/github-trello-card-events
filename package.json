{
  "name": "github-trello-card-events",
  "version": "2.0.1",
  "engines": {
    "node": "16.x || 18.x || 20.x"
  },
  "dependencies": {
    "@octokit/rest": "20.0.1",
    "axios": "1.4.0",
    "body-parser": "1.20.2",
    "errorhandler": "1.5.1",
    "express": "4.18.2",
    "lodash": "4.17.21",
    "moment-timezone": "0.5.43"
  },
  "devDependencies": {
    "@octokit/openapi-types": "18.0.0",
    "@types/errorhandler": "1.5.0",
    "@types/express": "4.17.17",
    "@types/lodash": "4.14.195",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "eslint": "8.45.0",
    "eslint-config-airbnb-base": "~15.0.0",
    "eslint-config-airbnb-typescript": "~17.1.0",
    "eslint-config-prettier": "~8.8.0",
    "eslint-plugin-import": "~2.27.5",
    "eslint-plugin-jsdoc": "~46.4.4",
    "eslint-plugin-prettier": "~5.0.0",
    "eslint-plugin-promise": "~6.1.1",
    "eslint-plugin-security": "~1.7.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "markdownlint-cli": "~0.35.0",
    "npm-run-all": "~4.1.5",
    "prettier": "3.0.0",
    "prettier-plugin-packagejson": "2.4.5",
    "typescript": "5.1.6"
  },
  "main": "dist/server.js",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "tsc",
    "lint:markdown": "prettier --write '*.md' '!(node_modules|dist)/**/*.md' && markdownlint '*.md' '!(node_modules|dist)/**/*.md' --config=.github/linters/.markdown-lint.yml --fix",
    "lint:code": "prettier --write *.json .eslintrc.js .prettierrc.js && eslint --fix --ext .ts src",
    "lint": "run-p lint:*",
    "lint-staged": "lint-staged",
    "dev": "tsc && node dist/server.js",
    "start": "node dist/server.js"
  },
  "lint-staged": {
    "*.md": ["prettier --write", "markdownlint --config=.github/linters/.markdown-lint.yml --fix"],
    "./*.js": ["eslint --fix"],
    "*.ts": ["eslint --fix"]
  },
  "repository": "github:github-trello-plugins/github-trello-card-events"
}
