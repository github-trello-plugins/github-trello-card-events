{
  "name": "github-trello-card-events",
  "version": "1.1.10",
  "engines": {
    "node": "16.x || 18.x"
  },
  "dependencies": {
    "@octokit/rest": "19.0.5",
    "axios": "1.2.1",
    "body-parser": "1.20.1",
    "errorhandler": "1.5.1",
    "express": "4.18.2",
    "lodash": "4.17.21",
    "moment-timezone": "0.5.39"
  },
  "devDependencies": {
    "@octokit/openapi-types": "14.0.0",
    "@types/errorhandler": "1.5.0",
    "@types/express": "4.17.14",
    "@types/lodash": "4.14.191",
    "@types/moment-timezone": "0.5.30",
    "@typescript-eslint/eslint-plugin": "5.45.1",
    "@typescript-eslint/parser": "5.45.1",
    "eslint": "8.29.0",
    "eslint-config-airbnb-base": "~15.0.0",
    "eslint-config-airbnb-typescript": "~17.0.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jsdoc": "~39.6.4",
    "eslint-plugin-prettier": "~4.2.1",
    "eslint-plugin-promise": "~6.1.1",
    "eslint-plugin-security": "~1.5.0",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "markdownlint-cli": "~0.32.2",
    "npm-run-all": "~4.1.5",
    "prettier": "2.8.0",
    "prettier-plugin-packagejson": "2.3.0",
    "typescript": "4.9.3"
  },
  "main": "dist/server.js",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "tsc",
    "lint:markdown": "prettier --write '*.md' '!(node_modules|dist)/**/*.md' && markdownlint '*.md' '!(node_modules|dist)/**/*.md' --config=.github/linters/.markdown-lint.yml --fix",
    "lint:code": "prettier --write *.json .eslintrc.js .prettierrc.js && eslint --fix --ext .ts src",
    "lint": "run-p lint:*",
    "lint-staged": "lint-staged",
    "dev": "tsc && node dist/server.js",
    "start": "node dist/server.js"
  },
  "lint-staged": {
    "*.md": ["prettier --write", "markdownlint --config=.github/linters/.markdown-lint.yml --fix"],
    "./*.js": ["eslint --fix"],
    "*.ts": ["eslint --fix"]
  },
  "repository": "https://github.com/mpirik/github-trello-card-events.git"
}
