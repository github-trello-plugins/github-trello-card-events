{
  "name": "github-trello-card-events",
  "version": "1.0.1",
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "@octokit/rest": "18.0.5",
    "body-parser": "1.19.0",
    "errorhandler": "1.5.1",
    "express": "4.17.1",
    "lodash": "4.17.20",
    "moment-timezone": "0.5.31",
    "request": "2.88.2",
    "request-promise": "4.2.6"
  },
  "devDependencies": {
    "@types/errorhandler": "1.5.0",
    "@types/express": "4.17.8",
    "@types/lodash": "4.14.161",
    "@types/moment-timezone": "0.5.30",
    "@types/request-promise": "4.1.46",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "eslint": "7.8.1",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsdoc": "30.3.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-security": "1.4.0",
    "husky": "3.1.0",
    "lint-staged": "10.3.0",
    "prettier": "2.1.1",
    "prettier-plugin-packagejson": "2.2.5",
    "typescript": "4.0.2"
  },
  "main": "dist/server.js",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "tsc",
    "lint": "prettier --write *.json .eslintrc.js .prettierrc.js && eslint --fix --ext .ts src",
    "dev": "tsc && node dist/server.js",
    "start": "node dist/server.js"
  },
  "lint-staged": {
    "./*.js": [
      "eslint --fix"
    ],
    "*.ts": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build && git add dist/"
    }
  },
  "repository": "https://github.com/mpirik/github-trello-card-events.git"
}
