{
  "name": "github-trello-card-events",
  "version": "1.1.0",
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@octokit/rest": "18.10.0",
    "axios": "0.21.4",
    "body-parser": "1.19.0",
    "errorhandler": "1.5.1",
    "express": "4.17.1",
    "lodash": "4.17.21",
    "moment-timezone": "0.5.33"
  },
  "devDependencies": {
    "@octokit/openapi-types": "10.2.1",
    "@types/errorhandler": "1.5.0",
    "@types/express": "4.17.13",
    "@types/lodash": "4.14.172",
    "@types/moment-timezone": "0.5.30",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "~14.2.1",
    "eslint-config-airbnb-typescript": "~14.0.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-import": "~2.24.2",
    "eslint-plugin-jsdoc": "~36.1.0",
    "eslint-plugin-prettier": "~4.0.0",
    "eslint-plugin-promise": "~5.1.0",
    "eslint-plugin-security": "~1.4.0",
    "husky": "7.0.2",
    "lint-staged": "11.1.2",
    "markdownlint-cli": "~0.28.1",
    "npm-run-all": "~4.1.5",
    "prettier": "2.4.0",
    "prettier-plugin-packagejson": "2.2.12",
    "typescript": "4.4.3"
  },
  "main": "dist/server.js",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "tsc",
    "lint:markdown": "prettier --write '*.md' '!(node_modules|dist)/**/*.md' && markdownlint '*.md' '!(node_modules|dist)/**/*.md' --config=.github/linters/.markdown-lint.yml --fix",
    "lint:code": "prettier --write *.json .eslintrc.js .prettierrc.js && eslint --fix --ext .ts src",
    "lint": "run-p lint:*",
    "lint-staged": "lint-staged",
    "dev": "tsc && node dist/server.js",
    "start": "node dist/server.js"
  },
  "lint-staged": {
    "*.md": [
      "markdownlint --config=.github/linters/.markdown-lint.yml --fix"
    ],
    "./*.js": ["eslint --fix"],
    "*.ts": ["eslint --fix"]
  },
  "repository": "https://github.com/mpirik/github-trello-card-events.git"
}
